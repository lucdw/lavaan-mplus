



        TITLE:	this is an example of a SEM with
        	continuous factor indicators
        DATA:	FILE IS ex5.11.dat;
        VARIABLE:	NAMES ARE y1-y12;
        analysis: convergence = 0.0000001;
        MODEL:	f1 BY y1-y3;
        	f2 BY y4-y6;
        	f3 BY y7-y9;
        	f4 BY y10-y12;
        	f4 ON f3;
        	f3 ON f1 f2;
        output: standardized;












